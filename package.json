{
  "name": "electron-cesium",
  "private": true,
  "version": "0.0.0",
  "main": "build/electron/electron.js",
  "scripts": {
    "clean": "rimraf build dist",
    "start": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "npm run clean && vite build && tsc",
